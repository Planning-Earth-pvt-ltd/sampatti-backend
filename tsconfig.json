{
  "compilerOptions": {
    "target": "ES2020",                                  
    "module": "commonjs",                               
    "rootDir": "./src",                                  
    "moduleResolution": "node",                    
    "outDir": "./dist",                                   
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": false                                 
  },
  "include":["src/**/*"],
  "exclude": ["node_modules" , "dist"]
}
